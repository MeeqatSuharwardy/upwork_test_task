{
  "name": "nango-nextjs-integration",
  "version": "1.0.0",
  "description": "A complete implementation of Nango in Next.js with integration examples",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "setup": "bash scripts/setup-nango.sh",
    "verify": "bash scripts/verify-setup.sh",
    "deploy:nango": "cd nango-integrations && nango deploy",
    "nango:start": "bash scripts/start-local-nango.sh",
    "nango:stop": "bash scripts/stop-local-nango.sh",
    "nango:logs": "docker-compose logs -f",
    "nango:restart": "docker-compose restart",
    "nango:status": "docker-compose ps",
    "docker:clean": "docker-compose down -v && docker system prune -f"
  },
  "dependencies": {
    "@nangohq/frontend": "^0.36.0",
    "@nangohq/node": "^0.36.0",
    "next": "14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3"
  }
}

